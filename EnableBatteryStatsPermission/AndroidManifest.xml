<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gsamlabs.xposed.mods.enablebatterystatspermission"
    android:versionCode="3"
    android:versionName="1.2" >

    <!-- Min SDK is 19 (KitKat) since this isn't needed prior.  To test it on
         an older level, just drop the min version down. -->
    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name">
        
        <meta-data android:value="true" android:name="xposedmodule"/>
        <meta-data android:value="2.4*" android:name="xposedminversion"/>
        <meta-data android:value="Enables battery monitor apps to retrieve Battery Statistics in KitKat" android:name="xposeddescription"/>    
    </application>

</manifest>
